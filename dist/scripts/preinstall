#!/bin/bash

if [[ -f "/Library/Fonts/FiraSystem-Light.ttf" ]]; then
    rm "/Library/Fonts/FiraSystem-Light.ttf"
fi

if [[ -f "/Library/Fonts/FiraSystem-Regular.ttf" ]]; then
    rm "/Library/Fonts/FiraSystem-Regular.ttf"
fi

if [[ -f "/Library/Fonts/FiraSystem-Medium.ttf" ]]; then
    rm "/Library/Fonts/FiraSystem-Medium.ttf"
fi

if [[ -f "/Library/Fonts/FiraSystem-Bold.ttf" ]]; then
    rm "/Library/Fonts/FiraSystem-Bold.ttf"
fi

if [[ -f "/Library/Fonts/FiraSystem.ttc" ]]; then
    rm "/Library/Fonts/FiraSystem.ttc"
fi

# Remove 10.11 version

if [[ -f "/Library/Fonts/FSText-Light.otf" ]]; then
    rm "/Library/Fonts/FSText-Light.otf"
fi

if [[ -f "/Library/Fonts/FSText-Regular.otf" ]]; then
    rm "/Library/Fonts/FSText-Regular.otf"
fi

if [[ -f "/Library/Fonts/FSText-RegularG1.otf" ]]; then
    rm "/Library/Fonts/FSText-RegularG1.otf"
fi

if [[ -f "/Library/Fonts/FSText-RegularG2.otf" ]]; then
    rm "/Library/Fonts/FSText-RegularG2.otf"
fi

if [[ -f "/Library/Fonts/FSText-RegularG3.otf" ]]; then
    rm "/Library/Fonts/FSText-RegularG3.otf"
fi

if [[ -f "/Library/Fonts/FSText-Medium.otf" ]]; then
    rm "/Library/Fonts/FSText-Medium.otf"
fi

if [[ -f "/Library/Fonts/FSText-Bold.otf" ]]; then
    rm "/Library/Fonts/FSText-Bold.otf"
fi

if [[ -f "/Library/Fonts/FSText-BoldG1.otf" ]]; then
    rm "/Library/Fonts/FSText-BoldG1.otf"
fi

if [[ -f "/Library/Fonts/FSText-BoldG2.otf" ]]; then
    rm "/Library/Fonts/FSText-BoldG2.otf"
fi

if [[ -f "/Library/Fonts/FSText-BoldG3.otf" ]]; then
    rm "/Library/Fonts/FSText-BoldG3.otf"
fi

if [[ -f "/Library/Fonts/FSText-Heavy.otf" ]]; then
    rm "/Library/Fonts/FSText-Heavy.otf"
fi


if [[ -f "/Library/Fonts/FSText-LightItalic.otf" ]]; then
    rm "/Library/Fonts/FSText-LightItalic.otf"
fi

if [[ -f "/Library/Fonts/FSText-RegularItalic.otf" ]]; then
    rm "/Library/Fonts/FSText-RegularItalic.otf"
fi

if [[ -f "/Library/Fonts/FSText-RegularItalicG1.otf" ]]; then
    rm "/Library/Fonts/FSText-RegularItalicG1.otf"
fi

if [[ -f "/Library/Fonts/FSText-RegularItalicG2.otf" ]]; then
    rm "/Library/Fonts/FSText-RegularItalicG2.otf"
fi

if [[ -f "/Library/Fonts/FSText-RegularItalicG3.otf" ]]; then
    rm "/Library/Fonts/FSText-RegularItalicG3.otf"
fi

if [[ -f "/Library/Fonts/FSText-MediumItalic.otf" ]]; then
    rm "/Library/Fonts/FSText-MediumItalic.otf"
fi

if [[ -f "/Library/Fonts/FSText-BoldItalic.otf" ]]; then
    rm "/Library/Fonts/FSText-BoldItalic.otf"
fi

if [[ -f "/Library/Fonts/FSText-BoldItalicG1.otf" ]]; then
    rm "/Library/Fonts/FSText-BoldItalicG1.otf"
fi

if [[ -f "/Library/Fonts/FSText-BoldItalicG2.otf" ]]; then
    rm "/Library/Fonts/FSText-BoldItalicG2.otf"
fi

if [[ -f "/Library/Fonts/FSText-BoldItalicG3.otf" ]]; then
    rm "/Library/Fonts/FSText-BoldItalicG3.otf"
fi

if [[ -f "/Library/Fonts/FSText-HeavyItalic.otf" ]]; then
    rm "/Library/Fonts/FSText-HeavyItalic.otf"
fi


exit 0
